<h1>Wall#index</h1>
<p>Find me in app/views/wall/index.html.erb</p>

<table>
    <tr>
        <th>Caption</th>
        <th>Image</th>
    </tr>
    <%= @posts.each do |post| %>
        <tr>
            <td><%= post.caption %></td>
            <td><%= post.image %></td>
        </tr>
    <% end %>
</table>